# Production Environment Variables for Vercel

# Sepolia Testnet Configuration
NEXT_PUBLIC_CONTRACT_ADDRESS=YOUR_SEPOLIA_CONTRACT_ADDRESS
NEXT_PUBLIC_CHAIN_ID=11155111
NEXT_PUBLIC_NETWORK_NAME=sepolia

# Infura RPC Endpoint for Sepolia
# Get your free API key at: https://infura.io
NEXT_PUBLIC_INFURA_API_KEY=YOUR_INFURA_KEY

# Wagmi Configuration
NEXT_PUBLIC_WAGMI_ENABLED=true

# WalletConnect (optional - will use default if not set)
# Get your project ID at: https://cloud.walletconnect.com
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=
